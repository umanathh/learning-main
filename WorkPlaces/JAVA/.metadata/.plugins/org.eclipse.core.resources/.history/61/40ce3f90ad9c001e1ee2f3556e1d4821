// Method to get prime number
	public boolean IsPrime(int n) {

		int n1 = n;
		int k = 0;

		if (n1 <= 1)
			return false;
		if (n > 1) {

			for (int i = 2; i <= Math.sqrt(n1); i++) {

				if (n % i == 0)
					k++;
			}

			if (k > 0)
				return false;
			else
				return true;

		}

	}

	public static void main(String[] args) {

		DataStructureDemo demo = new DataStructureDemo();

		int number = 45;
		demo.IsPrime(number);

	}
}